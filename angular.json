{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "form-controls": {
      "projectType": "library",
      "root": "projects/form-controls",
      "sourceRoot": "projects/form-controls/src",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/form-controls/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/form-controls/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/form-controls/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/form-controls/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "net-client": {
      "projectType": "library",
      "root": "projects/net-client",
      "sourceRoot": "projects/net-client/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/net-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/net-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/net-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/net-client/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "view-containers": {
      "projectType": "library",
      "root": "projects/view-containers",
      "sourceRoot": "projects/view-containers/src",
      "prefix": "vc",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/view-containers/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/view-containers/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/view-containers/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/view-containers/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "data-helpers": {
      "projectType": "library",
      "root": "projects/data-helpers",
      "sourceRoot": "projects/data-helpers/src",
      "prefix": "dh",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/data-helpers/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/data-helpers/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/data-helpers/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/data-helpers/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "reflective-data": {
      "projectType": "library",
      "root": "projects/reflective-data",
      "sourceRoot": "projects/reflective-data/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/reflective-data/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/reflective-data/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/reflective-data/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/reflective-data/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "playground": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/playground",
      "sourceRoot": "projects/playground/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/playground",
            "index": "projects/playground/src/index.html",
            "browser": "projects/playground/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/playground/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/playground/public"
              }
            ],
            "styles": [
              "projects/playground/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "playground:build:production"
            },
            "development": {
              "buildTarget": "playground:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/playground/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/playground/public"
              }
            ],
            "styles": [
              "projects/playground/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "editors": {
      "projectType": "library",
      "root": "projects/editors",
      "sourceRoot": "projects/editors/src",
      "prefix": "ed",
      "architect": {
        "build": {
